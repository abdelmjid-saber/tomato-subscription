{
    "name": "tomatophp/tomato-subscription",
    "type": "library",
    "description": "Plan subscription with selected features to build a feature control plan for Tomato",
    "keywords": [
        "php",
        "laravel",
        "subscription",
        "plans",
        "features",
        "tomato",
        "splade"
    ],
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "TomatoPHP\\TomatoSubscription\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "TomatoPHP\\TomatoSubscription\\TomatoSubscriptionServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Fady Mondy",
            "email": "info@3x1.io"
        }
    ],
    "require": {
        "php": "^8.0.2",
        "tomatophp/console-helpers": "^1.0"
    }
}
